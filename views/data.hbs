    <table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">Avatar</th>
      <th scope="col">Username</th>
      <th scope="col">XP</th>
      <th scope="col">Level</th>
      <th scope="col">message</th>
    </tr>
  </thead>
  <tbody>
      {{# each items }}
    <tr>
      <td><img src="{{this.avatarURL}}" class="lbplayericon rounded-circle" alt=""></td>
      <td>{{this.username}} </td>
      <td>{{this.xp}}</td>
      <td>{{this.level}}</td>
      <td>{{this.message}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
